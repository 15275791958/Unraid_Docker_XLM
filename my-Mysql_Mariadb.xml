<?xml version="1.0"?>
<Container version="2">
  <Name>Mysql_Mariadb</Name>
  <Repository>linuxserver/mariadb</Repository>
  <Registry>https://hub.docker.com/r/linuxserver/mariadb/</Registry>
  <Network>host</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/40737-support-linuxserverio-mariadb/</Support>
  <Project>https://mariadb.org/</Project>
  <Overview>An Enhanced drop in replacement for Mysql</Overview>
  <Category>Network:Other MediaApp:Other Other: Tools:</Category>
  <WebUI/>
  <TemplateURL>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/mariadb.xml</TemplateURL>
  <Icon>https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/mariadb-icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1624531996</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Description>An Enhanced drop in replacement for Mysql</Description>
  <Networking>
    <Mode>host</Mode>
    <Publish>
      <Port>
        <HostPort>3306</HostPort>
        <ContainerPort>3306</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/user/Appdata/Mariadb/</HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Environment>
    <Variable>
      <Value>0</Value>
      <Name>PUID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>0</Value>
      <Name>PGID</Name>
      <Mode/>
    </Variable>
    <Variable>
      <Value>Mysql273406++--</Value>
      <Name>MYSQL_ROOT_PASSWORD</Name>
      <Mode/>
    </Variable>
  </Environment>
  <Labels/>
  <Config Name="Host Port 1" Target="3306" Default="3306" Mode="tcp" Description="Container Port: 3306" Type="Port" Display="always" Required="true" Mask="false">3306</Config>
  <Config Name="Key 1" Target="PUID" Default="99" Mode="" Description="Container Variable: PUID" Type="Variable" Display="always" Required="false" Mask="false">0</Config>
  <Config Name="Key 2" Target="PGID" Default="100" Mode="" Description="Container Variable: PGID" Type="Variable" Display="always" Required="false" Mask="false">0</Config>
  <Config Name="Key 3" Target="MYSQL_ROOT_PASSWORD" Default="0" Mode="" Description="Container Variable: MYSQL_ROOT_PASSWORD&#13;&#10;Mysql273406++--" Type="Variable" Display="always" Required="false" Mask="false">Mysql273406++--</Config>
  <Config Name="AppData Config Path" Target="/config" Default="/mnt/user/Docker/mariadb" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false">/mnt/user/Appdata/Mariadb/</Config>
</Container>
